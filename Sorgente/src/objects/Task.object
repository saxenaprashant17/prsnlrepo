        <url>{!requireScript(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!requireScript(&quot;/soap/ajax/10.0/apex.js&quot;)}
var taskIds = {!GETRECORDIDS($ObjectType.Task)};
var opps = []; 
for(var i=0; i&lt;taskIds.length; i++){
  var op = new sforce.SObject(&quot;Task&quot;); 
  op.Id = taskIds[i]; 
  op.Status = &apos;Completed&apos;;
  opps.push(op);
}
if(opps.length &gt; 0){
  sforce.connection.update(opps);
  window.location.reload(true); 
}</url>
