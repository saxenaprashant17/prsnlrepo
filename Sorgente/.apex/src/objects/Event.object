    <validationRules>
        <fullName>Inserisci_area_di_interesse</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Valutazione_incontro__c,&quot;Interessato&quot;),ISNULL(Area_di_interesse__c))</errorConditionFormula>
        <errorDisplayField>Area_di_interesse__c</errorDisplayField>
        <errorMessage>Inserisci un&apos;area di interesse alla quale lo specialista è interessato. Puoi selezionarne più di una.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inserisci_motivazione_di_non_interesse</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Valutazione_incontro__c,&quot;Non interessato&quot;),ISPICKVAL(Motivazione_non_interesse__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Motivazione_non_interesse__c</errorDisplayField>
        <errorMessage>Inserisci una motivazione di non interesse</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inserisci_specifica_della_motivazione</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Valutazione_incontro__c,&quot;Non interessato&quot;),OR(ISPICKVAL(Motivazione_non_interesse__c ,&quot;Qualità servizio&quot;),ISPICKVAL(Motivazione_non_interesse__c ,&quot;Listino incompleto&quot;),ISPICKVAL(Motivazione_non_interesse__c ,&quot;Non crede nell&apos;approccio&quot;),ISPICKVAL(Motivazione_non_interesse__c ,&quot;Collabora con competitor&quot;),ISPICKVAL(Motivazione_non_interesse__c ,&quot;Altro&quot;)),ISBLANK(Area_note__c ))</errorConditionFormula>
        <errorDisplayField>Area_note__c</errorDisplayField>
        <errorMessage>Inserisci una specifica dei riguardo la motivazione</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inserisci_specifica_progetti</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Valutazione_incontro__c,&quot;Interessato&quot;),INCLUDES(Area_di_interesse__c,&quot;Progetti (siti web)&quot;), ISBLANK(Area_note__c ))</errorConditionFormula>
        <errorDisplayField>Area_note__c</errorDisplayField>
        <errorMessage>Inserisci una specifica dei progetti richiesti.</errorMessage>
    </validationRules>
